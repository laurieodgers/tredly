{
    "container": {
        "persistentStorage": {
            "identifier": "test",
            "mountPoint": "/test"
        },
        "firewall": {
            "allowPorts": {
                "tcp": {
                    "out": [
                        80,
                        443
                    ],
                    "in": [
                        4443
                    ]
                },
                "udp": {
                    "out": [
                        53
                    ],
                    "in": []
                }
            },
            "ipv4Whitelist": []
        },
        "startOrder": 1,
        "restrictTo": "",
        "technicalOptions": {},
        "customDNS": [
            "8.8.8.8",
            "8.8.4.4"
        ],
        "buildOptions": {
            "publish": false
        },
        "resourceLimits": {
            "maxCpu": 8,
            "maxRam": "1024M",
            "maxHdd": "1024M"
        },
        "proxy": {
            "layer7Proxy": [],
            "layer4Proxy": true
        },
        "replicate": false,
        "operations": {
            "onStop": [],
            "onCreate": [
                {
                    "value": "nginx",
                    "type": "installPackage"
                },
                {
                    "source": "partition/sslCerts/selfsigned",
                    "type": "fileFolderMapping",
                    "target": "/usr/local/etc/nginx/ssl"
                },
                {
                    "source": "/data",
                    "type": "fileFolderMapping",
                    "target": "/usr/local/www/"
                },
                {
                    "source": "/nginx.conf",
                    "type": "fileFolderMapping",
                    "target": "/usr/local/etc/nginx/"
                },
                {
                    "value": "chown -R www:www /usr/local/www/",
                    "type": "exec"
                },
                {
                    "value": "sysrc nginx_enable=YES",
                    "type": "exec"
                },
                {
                    "value": "service nginx start",
                    "type": "exec"
                }
            ]
        },
        "name": "test-www"
    }
}
